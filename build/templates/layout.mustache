<body class="{{#bodyclass}}{{class}}{{/bodyclass}}">
    <script type="text/javascript">document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    <div id="skiplink-container">
        <div>
            <a href="#content" class="skiplink">Skip to main content</a>
        </div>
    </div>

    <div id="global-cookie-message">
        <div class="outer-block">
            <div class="inner-block">
                <p>
                    We have placed cookies on your computer to help make this website better. You can change your cookies
                    settings at any time and can
                    <a href="">find out about cookies here</a>. If you continue using this site we will assume you accept
                    these cookies.
                </p>
            </div>
        </div>
    </div>

    <header role="banner" id="global-header">
        <div class="header-wrapper">
            <div class="header-global">
                <div class="header-logo">
                    <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" id="logo" class="content">
                        <img src="/assets/images/gov.uk_logotype_crown.png?0.10.0" width="35" height="31" alt=""> GOV.UK
                    </a>
                </div>
                <form id="search">
                  <input type="text" class="form-control" />
                  <input type="submit" id="search-submit" />
                </form>
            </div>
        </div>
    </header>

    <div id="global-header-bar">
        <div class="inner-block">
            <div class="header-bar"></div>
        </div>
    </div>

    <main role="main" id="wrapper">
        <div id="content">
            <div class="inner-block">
                <div class="phase-banner">
                    <p>
                        <strong class="phase-tag">ALPHA</strong>
                        <span>This is a new service â€“ your feedback will help us to improve it.</span>
                    </p>
                </div>
            </div>

        {{>content}}

        </div>
    </main>

    <script src="/assets/javascripts/govuk-template.js" type="text/javascript"></script>
    <script src="/assets/javascripts/jquery.min.js" ></script>
    <script src="/assets/javascripts/moment.min.js" ></script>
    <script src="/assets/javascripts/fullcalendar.js" ></script>
    <script src="/assets/javascripts/highcharts.js" ></script>

    <script src="/assets/javascripts/ucdsc.js" ></script></body>
</html>
